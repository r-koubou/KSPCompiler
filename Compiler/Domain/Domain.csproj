<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <LangVersion>10</LangVersion>
        <TargetFramework>netstandard2.1</TargetFramework>
        <Nullable>enable</Nullable>
        <Copyright>R-Koubou</Copyright>
        <Company>R-Koubou</Company>
        <Product>KSPCompiler.Domain</Product>
        <AssemblyName>KSPCompiler.Domain</AssemblyName>
        <RootNamespace>KSPCompiler.Domain</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\Commons\Commons.csproj" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Update="CompilerMessages\CompilerMessageResources.resx">
        <Generator>PublicResXFileCodeGenerator</Generator>
        <LastGenOutput>CompilerMessageResources.Designer.cs</LastGenOutput>
      </EmbeddedResource>
    </ItemGroup>

    <ItemGroup>
      <Compile Update="CompilerMessages\CompilerMessageResources.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>CompilerMessageResources.resx</DependentUpon>
      </Compile>
    </ItemGroup>

</Project>
